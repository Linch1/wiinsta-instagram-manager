<div class="page focus">

  <div class="top-container">
    <%include ../partials/focus_actions.ejs%>
    <div class="row">
      <header class="page-header">
        <div class="page-icon contain-image focus-icon"></div>
        <h1 class="page-title">Focus</h1>
      </header>
      <div class="column medium-2">


        <a class="help-link" href="#" data-reveal-id="focusModal">Click for help <div class="icon contain-image help-icon"></div></a>
        <div id="focusModal" class="reveal-modal" data-reveal aria-labelledby="modalTitle" aria-hidden="true" role="dialog">
          <div class="getting-started-guide focus-guide">
            <%include ../partials/start_guide_image.ejs%>
            <div class="guide-image" style="background-image: url('../../public/img/getting-started/focus/focus.jpg')"></div>
            <div class="guide-image" style="background-image: url('../../public/img/getting-started/images/images1.jpg')"></div>
            <div class="guide-image" style="background-image: url('../../public/img/getting-started/images/images2.jpg')"></div>
